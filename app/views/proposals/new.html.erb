<div class="container border">
	<div class="row">
		<!-- <div class="col-md-2"></div> -->
		<div class="col-md-10">
			<%= form_for @proposal do |f| -%>	
			<div class="">
	        	<div class="">
					<%= f.label :description, class:'' %>
				</div>				
	        	<div class="">
					<%= f.text_field :description, autofocus: true,class:"form-control form-control-lg", placeholder:"200 students with partents" %>
				</div>
			</div>
			<br>
			<div class="">
				<div class="">
					<%= f.label :reach, class:"" %>
				</div>
	        	<div class="">
					<%= f.text_field :reach,class:"form-control form-control-lg", placeholder:"reach" %>
				</div>
			</div>
			<br>
			<div class="">
				<div class="">
					<%= f.label :location, class:"" %>
				</div>
	        	<div class="">
					<%= f.text_field :location,class:"form-control form-control-lg", placeholder:"location" %>
				</div>
			</div>
			<br>
			<div class="">
				<div class="">
					<%= f.label :amount_goal, class:"" %>
				</div>
	        	<div class="">
					<%= f.text_field :amount_goal,class:"form-control form-control-lg", placeholder:"amount_goal" %>
				</div>
			</div>
			<br>
			<div class="row">
				<div class="col-md-3">
					<div class="">
						<%= f.label :date, class:"" %>
					</div>
		        	<div class="">
						<%= f.date_field :date,class:"form-control form-control-lg", placeholder:"date" %>
					</div>
				</div>
			</div>
			<br>
			<br>
			<div class="actions form-row form-group">
				<div class="col">
				  <%= f.submit "Submit", class:'btn btn-block btn-success btn-lg' %>
				</div>
			</div>
			<% end -%>	
		</div>
		
	</div>
</div>

